{
    "name": "arfit-kit-react-native",
    "version": "1.0.0",
    "description": "React Native bindings for ARFitKit - AR Virtual Try-On SDK",
    "main": "lib/commonjs/index",
    "module": "lib/module/index",
    "types": "lib/typescript/index.d.ts",
    "react-native": "src/index",
    "source": "src/index",
    "files": [
        "src",
        "lib",
        "android",
        "ios",
        "cpp",
        "*.podspec"
    ],
    "scripts": {
        "build": "bob build",
        "typescript": "tsc --noEmit",
        "lint": "eslint \"**/*.{js,ts,tsx}\"",
        "prepare": "bob build",
        "example": "yarn --cwd example"
    },
    "keywords": [
        "react-native",
        "ar",
        "virtual-try-on",
        "augmented-reality",
        "fashion"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/your-org/ARfit-kit.git"
    },
    "author": "ARfit-kit Contributors",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/your-org/ARfit-kit/issues"
    },
    "homepage": "https://github.com/your-org/ARfit-kit#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "devDependencies": {
        "@types/react": "^18.2.48",
        "@types/react-native": "^0.72.8",
        "react": "18.2.0",
        "react-native": "0.73.2",
        "react-native-builder-bob": "^0.23.2",
        "typescript": "^5.3.3"
    },
    "peerDependencies": {
        "react": "*",
        "react-native": "*"
    },
    "codegenConfig": {
        "name": "RNARFitKitSpec",
        "type": "modules",
        "jsSrcsDir": "src"
    },
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            "commonjs",
            "module",
            [
                "typescript",
                {
                    "project": "tsconfig.build.json"
                }
            ]
        ]
    }
}